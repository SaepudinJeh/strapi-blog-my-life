{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "singularName": "story",
    "pluralName": "stories",
    "displayName": "Story"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "acts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::act.act",
      "mappedBy": "story"
    }
  }
}
